{
  "name": "saml-oidc-test-app",
  "version": "1.0.0",
  "description": "Test application for SAML and OIDC authentication",
  "main": "index.js",
  "scripts": {
    "start": "cross-env HOST=0.0.0.0 react-scripts start",
    "start:server": "node server/index.js",
    "start:prod": "cross-env NODE_ENV=production node server/index.js",
    "build": "react-scripts build",
    "build:serve": "npm run build && npm run start:prod",
    "docker:build": "docker build -t saml-oidc-test-app .",
    "docker:run": "docker run -p 3001:3001 -v ./data:/app/data:ro saml-oidc-test-app",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "test": "jest",
    "test:watch": "jest --watch",
    "eject": "react-scripts eject"
  },
  "keywords": ["saml", "oidc", "authentication"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "react-scripts": "5.0.1",
    "express": "^4.18.2",
    "passport": "^0.7.0",
    "passport-saml": "^3.2.4",
    "passport-oauth2": "^1.7.0",
    "express-session": "^1.17.3",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.1.0",
    "xml-crypto": "^5.0.0",
    "xml2js": "^0.6.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^6.3.3",
    "babel-jest": "^29.7.0",
    "cross-env": "^7.0.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
